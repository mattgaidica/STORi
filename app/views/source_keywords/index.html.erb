<p id="notice"><%= notice %></p>

<h1>Source Keywords</h1>

<table>
  <thead>
    <tr>
      <th>Source</th>
      <th>Keyword</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @source_keywords.each do |source_keyword| %>
      <tr>
        <td><%= source_keyword.source %></td>
        <td><%= source_keyword.keyword %></td>
        <td><%= link_to 'Show', source_keyword %></td>
        <td><%= link_to 'Edit', edit_source_keyword_path(source_keyword) %></td>
        <td><%= link_to 'Destroy', source_keyword, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Source Keyword', new_source_keyword_path %>
